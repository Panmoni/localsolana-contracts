  "editor.tokenColorCustomizations": {
    "comments": "#A6A5B0",
    "strings": "#14F195",
    "keywords": "#9945FF",
    "numbers": "#FFD840",
    "types": "#6A45FF",
    "functions": "#14F195",
    "variables": "#F0F1FF",
    "textMateRules": [
      {
        "scope": "entity.name.tag",
        "settings": {
          "foreground": "#9945FF"
        }
      },
      {
        "scope": "entity.other.attribute-name",
        "settings": {
          "foreground": "#14F195"
        }
      },
      {
        "scope": "markup.heading",
        "settings": {
          "foreground": "#9945FF",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "markup.italic",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "markup.bold",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "markup.quote",
        "settings": {
          "foreground": "#14F195",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "markup.inline.raw",
        "settings": {
          "foreground": "#17E080"
        }
      },
      {
        "scope": "markup.fenced_code.block",
        "settings": {
          "foreground": "#A6A5B0"
        }
      }
    ]
  },
  "workbench.colorTheme": "Default Dark+",
  "workbench.iconTheme": "vs-seti",
  "editor.fontFamily": "'Source Code Pro Light', 'Inconsolata', 'Ubuntu Mono', monospace",
  "editor.fontSize": 18,
  "editor.fontWeight": "300",
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [100],
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "terminal.integrated.fontFamily": "'Inconsolata Light', 'Source Code Pro Light', monospace",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontWeight": "300",
  "window.title": "${dirty}${activeEditorShort}${separator}CelestialCode${separator}${rootName}",
  "editor.lineHeight": 1.5,
  "editor.letterSpacing": 0.5,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 100,
  "editor.minimap.showSlider": "always",
  "editor.guides.bracketPairs": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "editor.wordWrap": "on",
  "workbench.editor.wrapTabs": true,
  "breadcrumbs.enabled": true,
  "explorer.compactFolders": false,
  "editor.hideCursorInOverviewRuler": true,
  "window.zoomLevel": 1
}
